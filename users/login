		<script type="text/javascript">
	$(document).ready(function() {
		$('#ContentRegister').hide();
		$('ul.form-tabs li a:contains(Login)').parent().addClass('selected');

		$('ul.form-tabs li a').bind('click', function(e) {
			$('div.tab-content > div').hide();
			$('#Content' + $(this).text()).show();
			$('ul.form-tabs li').removeClass('selected');
			$(this).parent().addClass('selected');
			return false;
		});
	});
</script>

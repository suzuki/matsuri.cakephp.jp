	<div class="content" id="travel">
		<script type="text/javascript">
	var map = null;

	var markers = [
		{
			type: "airport",
			title: "Narita Airport (NRT)",
			description: "",
			lat: "35.773552",
			lng: "140.388244"
		}, {
			type: "hotel",
			title: "Shibuya Tokyu Inn",
			description: "Single 15,500yen, includes LAN",
			lat: "35.660203",
			lng: "139.701993"
		}, {
			type: "hotel",
			title: "Shibuya Granbell Hotel",
			description: "Single 9,100yen, includes BreakFast",
			lat: "35.656245",
			lng: "139.701637"
		}, {
			type: "hotel",
			title: "SAKURA FLEUR AOYAMA",
			description: "Single 8,800yen",
			lat: "35.659575",
			lng: "139.705748"
		}, {
			type: "hotel",
			title: "SHIBUYA CRESTON HOTEL",
			description: "Single 15,6922yen",
			lat: "35.663266",
			lng: "139.694699"
		}, {
			type: "venue",
			title: "SHIDAX Hall / CakeMatsuri Venue",
			description: 'Location of CakeMatsuri conference and workshop<br />Join us for great presentations, great food, great discussions and free wifi!',
			lat: "35.663128",
			lng: "139.700653"
			
		}
	];

	$(function() {
		$('#HotelList td.name').bind('click', function(e) {
			$('#HotelList td.name').parents("tr").next().hide();
			$(this).parents('tr').next().show();
		});

		$('a.map-link').click(function() {
			if ($(this).parents('tr').length > 0) {
				placeName = $(this).parents('tr').prev().find('td.name').html();
			} else {
				placeName = $(this).html()
			}

			for (n in markers) {
				if (markers[n].title != placeName) {
					continue;
				}

				window.coords = new GLatLng(markers[n].lat, markers[n].lng);
				window.currentMarker = markers[n];
				map.panTo(window.coords);

				setTimeout(
					function() {
						map.setCenter(window.coords, 14);
						window.currentMarker.object.openInfoWindowHtml(
							'<strong>' + window.currentMarker.title + '</strong>' +
							'<p>' + window.currentMarker.description + '</p>'
						);
					},
					700
				);
				break;
			}
		});

		var icons = {
			airport: new GIcon(
				G_DEFAULT_ICON,
				"/themed/tokyo09/img/map/airport-marker.png"
			),
			hotel: new GIcon(
				G_DEFAULT_ICON,
				"/themed/tokyo09/img/map/hotel-marker.png"
			),
			venue: new GIcon(
				G_DEFAULT_ICON,
				"/themed/tokyo09/img/map/venue-marker.png"
			)
		};
		icons.airport.imageSize = new GSize(50, 50);
		icons.hotel.imageSize = new GSize(50, 50);
		icons.venue.imageSize = new GSize(50, 50);

		map = new GMap2(document.getElementById("TravelMap"));

		map.setCenter(new GLatLng(35.66, 139.70), 14);
		map.addControl(new GSmallMapControl());
		map.enableContinuousZoom();

		GEvent.addListener(map, "click", function(overlay, latlng) {
			if (!overlay) {
				return;
			}
			for (n in markers) {
				if (markers[n].object != overlay) {
					continue;
				}
				m = markers[n];
				info = '<strong>' + m.title + '</strong><p>' + m.description + '</p>';
				overlay.openInfoWindowHtml(info);
				$('#HotelList td.name').parents("tr").next().hide();
				$('#HotelList td.name:contains("' + m.title + '")').parents('tr').next().show();
			}
		});

		GEvent.addListener(map, "tilesloaded", function() {
			$('#MapLoader').slideUp();

			$.each(markers, function(i, item) {
				marker = new GMarker(new GLatLng(item.lat, item.lng), { icon: icons[item.type] });
				markers[i].object = marker;
				map.addOverlay(marker);
			});
		});
	});
</script>

